    <Style TargetType="Page" ApplyToDerivedTypes="True">
        <Setter Property="Padding" Value="0"/>
        <Setter Property="BackgroundColor" Value="{AppThemeBinding Light={StaticResource ColorWhite}, Dark={StaticResource ColorOffBlack}}" />
    </Style>

    <Style TargetType="Shell" ApplyToDerivedTypes="True">
        <Setter Property="Shell.BackgroundColor" Value="{AppThemeBinding Light={StaticResource ColorWhite}, Dark={StaticResource ColorOffBlack}}" />
        <Setter Property="Shell.ForegroundColor" Value="{AppThemeBinding Light={StaticResource ColorBlack}, Dark={StaticResource ColorSecondaryDarkText}}" />
        <Setter Property="Shell.TitleColor" Value="{AppThemeBinding Light={StaticResource ColorBlack}, Dark={StaticResource ColorSecondaryDarkText}}" />
        <Setter Property="Shell.DisabledColor" Value="{AppThemeBinding Light={StaticResource ColorGray200}, Dark={StaticResource ColorGray950}}" />
        <Setter Property="Shell.UnselectedColor" Value="{AppThemeBinding Light={StaticResource ColorGray200}, Dark={StaticResource ColorGray200}}" />
        <Setter Property="Shell.NavBarHasShadow" Value="False" />
        <Setter Property="Shell.TabBarBackgroundColor" Value="{AppThemeBinding Light={StaticResource ColorWhite}, Dark={StaticResource ColorBlack}}" />
        <Setter Property="Shell.TabBarForegroundColor" Value="{AppThemeBinding Light={StaticResource ColorMagenta}, Dark={StaticResource ColorWhite}}" />
        <Setter Property="Shell.TabBarTitleColor" Value="{AppThemeBinding Light={StaticResource ColorMagenta}, Dark={StaticResource ColorWhite}}" />
        <Setter Property="Shell.TabBarUnselectedColor" Value="{AppThemeBinding Light={StaticResource ColorGray900}, Dark={StaticResource ColorGray200}}" />
    </Style>

    <Style TargetType="NavigationPage">
        <Setter Property="BarBackgroundColor" Value="{AppThemeBinding Light={StaticResource ColorWhite}, Dark={StaticResource ColorOffBlack}}" />
        <Setter Property="BarTextColor" Value="{AppThemeBinding Light={StaticResource ColorGray200}, Dark={StaticResource ColorWhite}}" />
        <Setter Property="IconColor" Value="{AppThemeBinding Light={StaticResource ColorGray200}, Dark={StaticResource ColorWhite}}" />
    </Style>

    <Style TargetType="TabbedPage">
        <Setter Property="BarBackgroundColor" Value="{AppThemeBinding Light={StaticResource ColorWhite}, Dark={StaticResource ColorGray950}}" />
        <Setter Property="BarTextColor" Value="{AppThemeBinding Light={StaticResource ColorMagenta}, Dark={StaticResource ColorWhite}}" />
        <Setter Property="UnselectedTabColor" Value="{AppThemeBinding Light={StaticResource ColorGray200}, Dark={StaticResource ColorGray950}}" />
        <Setter Property="SelectedTabColor" Value="{AppThemeBinding Light={StaticResource ColorGray950}, Dark={StaticResource ColorGray200}}" />
    </Style>

</ResourceDictionary>
